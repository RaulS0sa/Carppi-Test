    <script>
        function ButtonPress() {
            //id="SaveCard"
            //id="ProcessingCard"
            document.getElementById('SaveCard').style.backgroundColor = '#0000FF';
            document.getElementById('SaveCard').innerHTML = 'Procesando...';
            var button = document.getElementById('SaveCard');

            var cn = document.getElementById('cardNumber').value;
            var esds = document.getElementById('cardExpiry').value;
            var essdads = document.getElementById('cardCvc').value;
            //document.getElementById('ProcessingCard').style.display = 'block';
            //string number, string Expiry, string cvc
            $.ajax({
                type: "POST",
                url: "/CarppiDeliveryAddCard/UpdatePaymentMethod?number=" + cn + "&Expiry=" + esds + "&cvc=" + essdads,
                //data: '{SuperID: "' + result.paymentIntent.payment_method + '" }',
                data: '{}',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    console.log(response);
                    if (response != "Failure") {
                        try {

                            button.innerHTML = "Validada!, cierra la ventana";
                            button.style.backgroundColor = '#00FF00';

                            setTimeout(function () {
                                try {
                                    window.Android_BottomModal.DismissPaymentModal();
                                }
                                catch (err) { }
                            }, 1500);

                        }
                        catch (err) {

                        }
                    }
                    else
                    {
                            button.innerHTML = "Error!";
                        button.style.backgroundColor = '#FF0000';

                    }




                },
                failure: function (response) {

                      button.innerHTML = "Error!";
                        button.style.backgroundColor = '#FF0000';
                    alert(JSON.stringify(response));

                },
                error: function (response) {
                    button.innerHTML = "Error!";
                        button.style.backgroundColor = '#FF0000';
                    alert(JSON.stringify(response));

                }
            });
        }

    </script>
