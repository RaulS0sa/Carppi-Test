@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">

    <title>404 HTML Tempalte by Colorlib</title>

    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700,900" rel="stylesheet">

    <link type="text/css" rel="stylesheet" href="https://colorlib.com/etc/404/colorlib-error-404-10/css/style.css">


    <!--[if lt IE 9]>
              <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
              <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
            <![endif]-->
</head>
<body>
    <div id="notfound">
        <div class="notfound">
            <div class="notfound-404">
                <h1>Ok!</h1>
            </div>
            <h2>Puedes Cerrar esta ventana</h2>
            <p>parecese que todo esta saliendo de acuerdo al plan.</p>
            <!--
            <a href="#" onclick="document.getElementById('id01').style.display = 'block';//window.Android.ShowFacebookLogButton();">Log in</a>
                -->
        </div>
    </div>

    <div id="id01" class="w3-modal">
        <div class="w3-modal-content">
            <header class="w3-container w3-purple">
                <span onclick="document.getElementById('id01').style.display='none'"
                      class="w3-button w3-display-topright"> &times;</span>
                <h2> Mis Datos</h2>
            </header>
            <div class="w3-container">
                <img alt="Mi foto" style="max-height:200px;max-width:100%;padding-bottom:5%" />
                <button class="w3-purple" style="width:100%;
        border: none;
        color: white;
        padding: 15px 32px;
        text-align: center;
        text-decoration: none;
        display: inline-block;
        font-size: 16px;">
                    Actualiza Foto
                </button>

                <input style="width:100%;margin-top:5%;border:none;border-bottom: 2px solid purple;" type="text" placeholder="Mi Nombre" />

                <input style="width:100%;margin-top:5%;border:none;border-bottom: 2px solid purple;" type="text" placeholder="Apellido Paterno" />

                <input style="width:100%;margin-top:5%;border:none;border-bottom: 2px solid purple;" type="text" placeholder="Correo" />

                <input style="width:100%;margin-top:5%;border:none;border-bottom: 2px solid purple;" type="text" placeholder="Telefono" />


                <div class="row" style="width:100%;margin-top:5%">
                    <div class="col-md-4">
                        Disponible Para tutorias
                    </div>
                    <div class="col-md-7">
                        <label class="switch">
                            <input type="checkbox" checked>
                            <span class="slider round"></span>
                        </label>
                    </div>
                </div>

            </div>
            <footer class="w3-container w3-purple">

                <button style="width:100%;  background-color: #222222;
        border: none;
        color: white;
        padding: 15px 32px;
        text-align: center;
        text-decoration: none;
        display: inline-block;
        font-size: 16px;">
                    Guardar
                </button>
                <button class="w3-purple" style="width:100%;
        border: none;
        color: white;
        padding: 15px 32px;
        text-align: center;
        text-decoration: none;
        display: inline-block;
        font-size: 16px;">
                    Cerrar
                </button>
            </footer>
        </div>
    </div>

    <script type="text/javascript" async="" src="https://www.google-analytics.com/analytics.js"></script>
    <script async="" src="https://www.googletagmanager.com/gtag/js?id=UA-23581568-13" type="text/javascript"></script>



</body>
</html>