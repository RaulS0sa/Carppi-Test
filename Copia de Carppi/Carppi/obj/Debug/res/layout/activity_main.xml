    <android.webkit.WebView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/webView_Bottomsheet" />
    </LinearLayout>

</androidx.coordinatorlayout.widget.CoordinatorLayout>
